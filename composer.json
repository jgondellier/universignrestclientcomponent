{
  "name": "universign/rest-client-component",
  "description": "L'Universign Rest Client Component est responsable de la communication avec Universign",
  "type": "library",
  "license": "MIT",
  "keywords": ["universign","client", "component"],
  "authors": [
    {
      "name": "Gondellier Jonathan"
    }
  ],
  "require": {
    "php": ">=7.4",
    "eightpoints/guzzle-bundle": "^8.5",
    "psr/log": "~1.1",
    "symfony/serializer": "~4.4",
    "symfony/property-access": "^4.4",
    "giggsey/libphonenumber-for-php": "^8.12",
    "monolog/monolog": "^2.9.2",
    "symfony/dotenv": "^4.4",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Universign\\Rest\\ClientComponent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
